{
  "build": {
    "groups": {
      "default": {
        "dest": "default",
        "moniker_range": "{moniker_name}"
      }
    },
    "content": [{
        "src": "structured/default",
        "files": [
          "**.yml"
        ],
        "exclude": [
          "toc.yml"
        ],
        "group": "default",
        "dest": "api"
      },
      {
        "src": "structured/default",
        "files": [
          "toc.md",
          "toc.yml"
        ],
        "exclude": [],
        "group": "default",
        "dest": "api/{product_name}"
      },
      {
        "src": ".",
        "files": [
          "index.md"
        ],
        "exclude": [
          "README.md"
        ],
        "group": "default",
        "dest": "api/{product_name}"
      }
    ],
    "resource": [{
      "files": [
        "**/*.png",
        "**/*.jpg"
      ],
      "exclude": [
        "**/obj/**",
        "**/includes/**",
        "_themes/**",
        "_themes.pdf/**"
      ]
    }],
    "pairing": [
      {
        "contentFolder": "structured",
        "overwriteFragmentsFolder": "authored"
      }
    ], 
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "rest_product": "powerappsdocs",
      "titleSuffix": "powerappsdocs REST API",
	  "author":"v-jutao",
	  "ms.author":"v-jutao",
	  "ms.topic":"reference",
	  "ms.devlang":"rest-apis",
	  "ms.date":"09/04/2019",
      "extendBreadcrumb": true,
      "feedback_system": "None"
    },
    "fileMetadata": {
      "searchScope": {
        "structured/**.yml": ["powerappsdocs", "REST"]
      }
    },
    "template": [],
    "dest": "rest-ref-template",
    "markdownEngineName": "markdig"
  }
}